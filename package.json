{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-s \"build:*\"",
    "build:css": "pnpm run generate:css -- --minify",
    "build:remix": "remix build",
    "lint": "run-s \"lint:*\"",
    "lint:eslint": "eslint app/*.tsx app/**/*.tsx app/**/*.ts",
    "lint:pretty": "prettier --write app/*.tsx app/**/*.tsx app/**/*.ts",
    "dev": "npm-run-all build --parallel \"dev:*\"",
    "dev:css": "pnpm run generate:css -- --watch",
    "dev:migrate": "prisma migrate dev --name seemless_update",
    "generate:css": "npx tailwindcss -i ./styles/tailwind.css -o ./app/tailwind.css",
    "dev:node": "cross-env NODE_ENV=development nodemon --require dotenv/config ./server.js --watch ./server.js",
    "dev:remix": "remix watch",
    "start": "cross-env NODE_ENV=production node ./server.js",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@prisma/client": "^4.15.0",
    "@remix-run/express": "^1.17.0",
    "@remix-run/node": "^1.17.0",
    "@remix-run/react": "^1.17.0",
    "compression": "^1.7.4",
    "config": "^3.3.9",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "express": "^4.18.2",
    "helmet": "^6.2.0",
    "isbot": "^3.6.10",
    "morgan": "^1.10.0",
    "pino": "^8.14.1",
    "pino-http": "^8.3.3",
    "pino-pretty": "^10.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-player": "^2.12.0"
  },
  "devDependencies": {
    "@alex-taxiera/eslint-config-ts": "^2.0.1",
    "@remix-run/dev": "^1.17.0",
    "@remix-run/eslint-config": "^1.17.0",
    "@types/react": "^18.2.12",
    "@types/react-dom": "^18.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "dotenv": "^16.1.4",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-modules-newline": "^0.0.6",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-standard": "^5.0.0",
    "nodemon": "^2.0.22",
    "npm-run-all": "^4.1.5",
    "prisma": "^4.15.0",
    "tailwindcss": "^3.3.2",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": ">=14"
  }
}